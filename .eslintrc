{
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true
  },
  "settings": {
    "import/resolver": {
      "alias": [
        ["~components", ".src/components"],
        ["~images", ".src/images"],
        ["~lib", ".src/lib"]
      ]
    }
  }
}
