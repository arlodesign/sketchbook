    "favicons-webpack-plugin": "^3.0.1",
    "file-loader": "^6.0.0",
    "gh-pages": "^3.1.0",
    "glob": "^7.1.6",
    "glsl-easings": "^1.0.0",
    "glsl-hsl2rgb": "^1.1.0",
    "glsl-noise": "^0.0.0",
